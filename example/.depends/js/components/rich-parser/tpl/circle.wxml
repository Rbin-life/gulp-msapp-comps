<import src="./video.wxml"/>
<import src="./img.wxml"/>
<import src="./text.wxml"/>
<import src="./br.wxml"/>

<template name="circle">
    <!--判断是否是标签节点-->
    <block wx:if="{{ item.node == 'element' }}">
        <block wx:if="{{item.tag == 'button'}}">
            <button type="default" size="mini">
                <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
                    <template is="circle_{{item.level}}" data="{{item}}" />
                </block>
            </button>
        </block>
        <!--li类型-->
        <block wx:elif="{{item.tag == 'li'}}">
            <view class="{{item.classStr}}" style="{{item.styleStr}}">
                <view class="{{item.classStr}}">
                    <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
                        <template is="circle_{{item.level}}" data="{{item}}" />
                    </block>
                </view>
            </view>
        </block>

        <!--video类型-->
        <block wx:elif="{{item.tag == 'video'}}">
            <template is="video" data="{{item}}" />
        </block>

        <!--img类型-->
        <block wx:elif="{{item.tag == 'img'}}">
            <template is="img" data="{{item}}" />
        </block>

        <!--a类型-->
        <block wx:elif="{{item.tag == 'a'}}">
            <view class="{{item.classStr}}" style="{{item.styleStr}}">
            <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
                <template is="circle_{{item.level}}" data="{{item}}" />
            </block>
        </view>
        </block>
        <block wx:elif="{{item.tag == 'table'}}">
            <view class="{{item.classStr}}" style="{{item.styleStr}}">
                <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
                    <template is="circle_{{item.level}}" data="{{item}}" />
                </block>
            </view>
        </block>

        <block wx:elif="{{item.tag == 'br'}}">
            <template is="br"></template>
        </block>
        <!--其他块级标签-->
        <block wx:elif="{{item.tagType == 'block'}}">
            <view class="rich-{{item.tag}} {{item.classStr}}" style="{{item.styleStr}}">
                <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
                    <template is="circle_{{item.level}}" data="{{item}}" />
                </block>
            </view>
        </block>

        <!--内联标签-->
        <view wx:else class="rich-{{item.tag}} {{item.classStr}}" style="{{item.styleStr}}">
            <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
                <template is="circle_{{item.level}}" data="{{item}}" />
            </block>
        </view>

    </block>

    <!--判断是否是文本节点-->
    <block wx:elif="{{ item.node == 'text' }}">
        <!--如果是，直接进行-->
        <template is="text" data="{{item}}" />
    </block>

</template>

