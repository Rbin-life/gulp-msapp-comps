.wgui-btn_disabled{border:none;color:#b2b2b2;background:#fff;background:-webkit-gradient(linear,left top,left bottom,color-stop(0,#fff),color-stop(1,#fff));box-shadow:0 0 2rpx #828282 inset}
.wgui-btn_sm{padding:0;width:116rpx;height:58rpx;line-height:58rpx}
.wgui-btn_lg{display:block;width:100%;height:82rpx;line-height:82rpx;font-size:32rpx;text-align:center;padding:0}
.wgui-btn_disabled:active{border:none;color:#b2b2b2;background-color:#fff;background:#fff;background:-webkit-gradient(linear,left top,left bottom,color-stop(0,#fff),color-stop(1,#fff));box-shadow:0 0 2rpx #828282 inset}
.wgui-btn_primary:active{border:none;color:#69CE68;background:#05aa04;background:-webkit-gradient(linear,left top,left bottom,color-stop(0,#05ac04),color-stop(1,#05a704));box-shadow:0 0 2rpx #168e15 inset}
.wgui-btn_primary.wgui-btn_disabled{background:#06bd04;background:-webkit-gradient(linear,left top,left bottom,color-stop(0,#06c004),color-stop(1,#06ba04));box-shadow:0 0 2rpx #199d18 inset;color:#fff;opacity:0.5}
.wgui-btn_link:active{color:#69CE68;background:#05aa04;background:-webkit-gradient(linear,left top,left bottom,color-stop(0,#05ac04),color-stop(1,#05a704));box-shadow:0 0 2rpx #168e15 inset}
.wgui-btn_link.wgui-btn_disabled{box-shadow:none;border-radius:0;background-clip:padding-box;background:0 0}
.wgui-icon{display:inline-block}
view,view:after,view:before{box-sizing:border-box}
html,body{height:100%}
page{height:100%;background-color:#ffffff;font-family:-apple-system,BlinkMacSystemFont,PingFang SC,Microsoft YaHei,sans-serif}
view{display:block}
@import "gifts_fix.wxss";